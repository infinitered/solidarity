{
  "name": "solidarity",
  "version": "0.1.0",
  "description": "Make sure all React Native dependencies are uniform across machines",
  "bin": {
    "solidarity": "bin/solidarity"
  },
  "scripts": {
    "start": "node bin/solidarity",
    "test": "tsc && ava",
    "snapupdate": "ava --update-snapshots",
    "serve:docs": "docsify serve docs"
  },
  "author": "Gant Laborde",
  "license": "MIT",
  "dependencies": {
    "gluegun": "^2.0.0-alpha.2",
    "minimist": "^1.2.0",
    "ramda": "^0.24.1",
    "typescript": "^2.5.3"
  },
  "devDependencies": {
    "@types/node": "^8.0.33",
    "ava": "^0.22.0",
    "docsify-cli": "^4.1.10",
    "execa": "^0.8.0",
    "tempy": "^0.2.1"
  },
  "ava": {
    "files": [
      "tests/**/*.{js,ts}",
      "!tests/testHelpers.js"
    ]
  }
}
